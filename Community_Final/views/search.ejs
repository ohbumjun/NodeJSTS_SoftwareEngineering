        <div class="row">
          <!-- 후기 게시판  -->
          <h3></h3>
          <% search.forEach((data) => { %>
              <div class="col-lg-4 col-md-6" data-search >
                <div class="icon-box" data-aos="fade-up">
                  <div class="icon"><i class="bi bi-briefcase"></i></div>
                  <h4 class="title">
                    <a href="/community/feedbackboard/post/<%= data.post_id %>">
                      <%= data.title %>
                    </a>
                  </h4>
                  <p class="description">
                    <%= data.content %>
                  </p>
                </div>
              </div>
            <%}) %>
        </div>
